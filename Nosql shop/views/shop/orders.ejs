<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
        		<% if (orders.length > 0)  {%>
		<ul>
		<%orders.forEach(o => {%>
		
		<li>
		<h1><%=o._id%></h1>
		<%o.items.forEach(item => {%>
		<li><%=item.name%> (<%=item.quantity%>)</li>
		<%})%>
		</li>
		<%})%>
		</ul>
		<% } else  {%>
		<h1>No order yet</h1>
			<% }%>
        </main>
        <%- include('../includes/end.ejs') %>